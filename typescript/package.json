{
  "name": "@lukewalczak/material-color-utilities",
  "version": "0.2.0",
  "type": "module",
  "description": "Algorithms and utilities that power the Material Design 3 (M3) color system, including choosing theme colors from images and creating tones of colors; all in a new color space.",
  "keywords": [
    "material",
    "material design",
    "material3",
    "material you",
    "color extraction",
    "theming"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/lukewalczak/material-color-utilities.git",
    "directory": "typescript"
  },
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/lukewalczak/material-color-utilities/issues"
  },
  "homepage": "https://github.com/lukewalczak/material-color-utilities/tree/main/typescript",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "unbuild": {
    "entries": [
      "./index"
    ],
    "clean": true,
    "declaration": true,
    "rollup": {
      "emitCJS": true
    }
  },
  "scripts": {
    "build": "unbuild",
    "watch": "unbuild --stub"
  },
  "devDependencies": {
    "@types/jasmine": "^3.10.6",
    "jasmine": "^4.0.2",
    "unbuild": "^0.8.8"
  }
}